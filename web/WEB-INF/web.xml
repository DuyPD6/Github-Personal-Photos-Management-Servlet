<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>controller.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminListImageServlet</servlet-name>
        <servlet-class>controller.AdminListImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminListUserServlet</servlet-name>
        <servlet-class>controller.AdminListUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>controller.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAccounteServlet</servlet-name>
        <servlet-class>controller.DeleteAccounteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhotoServlet</servlet-name>
        <servlet-class>controller.PhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageByUserServlet</servlet-name>
        <servlet-class>controller.ImageByUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>controller.SignUpServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteImageServlet</servlet-name>
        <servlet-class>controller.DeleteImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostImageServlet</servlet-name>
        <servlet-class>controller.PostImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchImagesServlet</servlet-name>
        <servlet-class>controller.SearchImagesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhotoDetailServlet</servlet-name>
        <servlet-class>controller.PhotoDetailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminListImageServlet</servlet-name>
        <url-pattern>/adminlistsevlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminListUserServlet</servlet-name>
        <url-pattern>/adminlistuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAccounteServlet</servlet-name>
        <url-pattern>/deleteaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhotoServlet</servlet-name>
        <url-pattern>/photo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageByUserServlet</servlet-name>
        <url-pattern>/imagebyuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteImageServlet</servlet-name>
        <url-pattern>/deleteimage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostImageServlet</servlet-name>
        <url-pattern>/postimageservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchImagesServlet</servlet-name>
        <url-pattern>/searchimages</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhotoDetailServlet</servlet-name>
        <url-pattern>/photoDetail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
